{
    "name": "scarcity",
    "type": "module",
    "version": "0.0.3",
    "description": "Powerful, type-inferred, and hyper-minimalistic library for server request propagation and dependency injection using a novel Supply Chain Architecture",
    "main": "dist/index.cjs",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "author": "Scarcity",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/scarcity-js/scarcity.git"
    },
    "imports": {
        "#*": "./src/*.ts"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsup",
        "prepublishOnly": "pnpm run build",
        "test": "vitest",
        "test:run": "vitest run"
    },
    "devDependencies": {
        "tsup": "^8.5.0",
        "turbo": "^2.5.5",
        "typescript": "latest",
        "vitest": "^3.2.4"
    },
    "dependencies": {
        "memoize": "^10.1.0"
    }
}
