{
    "name": "typearch",
    "type": "module",
    "version": "0.0.0",
    "description": "First fully type-inferred SOLID architecture framework for Typescript",
    "main": "dist/index.cjs",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "sideEffects": false,
    "author": "Typearch",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/typearch-js/typearch.git"
    },
    "imports": {
        "#*": "./src/*.ts"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "test": "vitest --typecheck",
        "test:run": "vitest run --typecheck",
        "typecheck": "vitest run --typecheck.only"
    },
    "devDependencies": {
        "@config.typearch/eslint": "*",
        "@config.typearch/typescript": "*",
        "memoize": "^10.1.0",
        "tsup": "^8.5.0",
        "turbo": "^2.5.5",
        "vitest": "^3.2.4"
    }
}
